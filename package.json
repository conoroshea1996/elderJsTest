{
  "name": "elderjs-template",
  "version": "1.3.2",
  "description": "This is a project template for [Elder.js](https://elderguide.com/tech/elderjs/) apps. The template lives at https://github.com/elderjs/template and the Elder.js source is here: https://github.com/elderjs/elderjs",
  "main": "./src/build.js",
  "scripts": {
    "start": "npm run dev",
    "build": "node ./src/cleanPublic.js && npm run build:rollup && npm run build:html",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "prebuild": "cd netlify/functions/getBlogs && npm i",
    "postbuild": "mkdirp netlify/functions/getBlogs/public/ && ncp elder.config.prod.js netlify/functions/getBlogs/elder.config.js && ncp src netlify/functions/getBlogs/src && ncp ___ELDER___ netlify/functions/getBlogs/___ELDER___ && ncp public/_elderjs netlify/functions/getBlogs/public/_elderjs && mkdir netlify/functions/getBlogs/images && ncp images/ejs-image-manifest.json netlify/functions/getBlogs/images/ejs-image-manifest.json",
    "dev": "rollup  -c -w --no-watch.clearScreen",
    "serve": "node ./src/cleanPublic.js && npm run build:rollup && SET NODE_ENV=production node ./src/server.js",
    "esbuild": "node esbuild",
    "eslint": "eslint --ext .js .",
    "eslint:fix": "eslint --fix --ext .js .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@elderjs/elderjs": "^1.5.5",
    "@elderjs/plugin-browser-reload": "^0.1.37",
    "@elderjs/plugin-images": "^1.1.6",
    "@elderjs/plugin-markdown": "^0.1.37",
    "@elderjs/plugin-seo-check": "0.1.37",
    "@elderjs/plugin-sitemap": "^1.1.6",
    "@tailwindcss/typography": "^0.4.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "elderjs-plugin-blog-pagination": "^0.7.0",
    "elderjs-plugin-i18n": "0.0.3",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "intersection-observer": "^0.12.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.1",
    "polka": "^0.5.2",
    "serverless-http": "^2.7.0",
    "sirv": "^1.0.11",
    "sitemap-generator": "^8.5.1",
    "svelte": "^3.38.3",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^2.2.4",
    "vanilla-lazyload": "^17.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.8",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.41.1"
  },
  "author": "Nick Reese",
  "license": "MIT",
  "files": [
    "index.js",
    "elderjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elderjs/template.git"
  },
  "keywords": [
    "svelte",
    "elderjs",
    "ssr",
    "static",
    "site",
    "generator",
    "seo",
    "ssg",
    "template",
    "partial",
    "hydration"
  ],
  "bugs": {
    "url": "https://github.com/Elderjs/template/issues"
  },
  "homepage": "https://github.com/Elderjs/template#readme"
}
